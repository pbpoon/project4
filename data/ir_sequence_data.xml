<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for purchase.order -->
        <record id="seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order</field>
            <field name="prefix">PO%(year)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--invoice.order-->
        <record id="seq_invoice_order" model="ir.sequence">
            <field name="name">Invoice Order</field>
            <field name="code">invoice.order</field>
            <field name="prefix">INV%(year)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
